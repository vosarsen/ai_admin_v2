REDIS_PASSWORD=

# WhatsApp (Baileys)
USE_PAIRING_CODE=true
WHATSAPP_PHONE_NUMBER=79936363848
WHATSAPP_MAX_QR_ATTEMPTS=3
WHATSAPP_MONITOR_ENABLED=false

# WhatsApp (Venom-bot) - Legacy
VENOM_SERVER_URL=http://localhost:3001
VENOM_API_KEY=
VENOM_SECRET_KEY=

# YClients API
YCLIENTS_BEARER_TOKEN=
YCLIENTS_USER_TOKEN=
YCLIENTS_PARTNER_ID=
YCLIENTS_COMPANY_ID=962302

# AI Provider Selection
AI_PROVIDER=deepseek
# Options: deepseek, gemini-flash, gemini-pro, gemini-flash-lite

# AI Service (DeepSeek)
DEEPSEEK_API_KEY=
DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_TEMPERATURE=0.7
DEEPSEEK_MAX_TOKENS=1500

# AI Service (Google Gemini)
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash
# Options: gemini-2.5-flash, gemini-2.5-pro, gemini-2.5-flash-lite

# Database (Supabase - Legacy)
SUPABASE_URL=
SUPABASE_KEY=

# Timeweb PostgreSQL (новый)
POSTGRES_HOST=192.168.0.4
POSTGRES_PORT=5432
POSTGRES_DATABASE=default_db
POSTGRES_USER=gen_user
POSTGRES_PASSWORD=
POSTGRES_MAX_CONNECTIONS=3

# Database mode (true = Supabase, false = Timeweb PostgreSQL)
USE_LEGACY_SUPABASE=true

# Sentry Error Tracking
SENTRY_DSN=
SENTRY_ENABLED=true
SENTRY_TRACES_SAMPLE_RATE=0.1
SENTRY_PROFILES_SAMPLE_RATE=0.1

# Queue Settings
MAX_CONCURRENT_WORKERS=3
MESSAGE_QUEUE_NAME=whatsapp-messages
REMINDER_QUEUE_NAME=reminders

# Business Rules
REMINDER_ADVANCE_HOURS=2
REMINDER_DAY_BEFORE_TIME=20:00
MAX_BOOKING_DAYS_AHEAD=30
MIN_BOOKING_MINUTES_AHEAD=30

# Security (Optional - for secrets encryption)
# Generate with: node scripts/manage-secrets.js generate-key
MASTER_KEY=
SECRETS_PATH=.secrets